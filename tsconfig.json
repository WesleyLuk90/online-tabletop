    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "outDir": "dist",
        "rootDir": "."
    }
}
